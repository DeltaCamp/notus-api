<section>
  <h3>{{title}} occurred at block {{block.number}}</h3>
  <ul>
    {{#matchers}}
      <li>{{sourceTitle}}: <b>{{sourceValue}}</b></li>
    {{/matchers}}
  </ul>
  <p>
    <a href="{{#etherscanTx}}{{transaction.hash}}{{/etherscanTx}}">View the transaction on Etherscan</a>
  </p>
</section>